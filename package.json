{
  "name": "json2xls-cli",
  "version": "1.0.0",
  "type": "module",
  "description": "json转excel和excel转json工具",
  "author": "zhuzesen",
  "bin": {
    "json2xls-cli": "./dist/esm/index.mjs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "json2xls": "node ./dist/esm/index.mjs i18nJson2xls",
    "json2xls2": "node ./dist/esm/index.mjs i18nJson2xls -i lang -o locale1 -l zh en id -n 国际化",
    "xls2json": "node ./dist/esm/index.mjs xls2i18nJson -i locale1/国际化.xlsx -o locale1 ",
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "inquirer": "^9.2.8",
    "inquirer-fuzzy-path": "^2.3.0",
    "json2xls": "^0.1.2",
    "node": "^20.5.0",
    "node-xlsx": "^0.23.0",
    "picocolors": "^1.0.0",
    "unzip": "^0.1.11",
    "xlsx": "^0.18.5"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/inquirer-fuzzy-path": "^2.3.6",
    "@types/node": "^20.4.5",
    "@types/pkginfo": "^0.4.0",
    "@types/unzip": "^0.1.1",
    "commander": "^11.0.0",
    "pkginfo": "^0.4.1"
  }
}
